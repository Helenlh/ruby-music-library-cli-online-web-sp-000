module Findable::module ClassMethods
  
  
  module Concerns::ModuleName
  # Module code here
  
  def find_by_name(name)
    self.all.detect {|a| a. name}
    end 
  end 
    
    
  def self.find_or_create_by_name(item)
    self.find(item) ? self.find(item) : self.new(item) 
    end 
  end 
end 

